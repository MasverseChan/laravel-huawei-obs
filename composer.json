{
    "name": "vccmas/huawei-obs-storage",
    "description": "huawei obs filesystem storage for laravel 10+",
    "keywords": ["huawei", "obs", "laravel", "filesystems", "storage"],
    "homepage": "https://github.com/MasverseChan/laravel-huawei-obs.git",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "vcc\\HuaweiObsStorage\\": "src/"
        }
    },
    "authors": [
        {
            "name": "vccmas",
            "email": "Chan.VS@masverse.com.my"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/filesystem": "9.*|10.*",
        "illuminate/support": "9.*|10.*",
        "league/flysystem": "3.*.*",
        "nesbot/carbon": "2.*.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "vccmas\\HuaweiObsStorage\\Providers\\HuaweiObsServiceProvider"
            ]
        }
    }
}
